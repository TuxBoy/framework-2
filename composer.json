{
    "name": "stratify/framework",
    "description": "Stratify framework package",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Stratify\\Framework\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Stratify\\Framework\\Test\\": "tests"
        }
    },
    "require": {
        "php": ">=7.0",
        "stratify/http": "~0.1.0",
        "stratify/router": "~0.1.0",
        "stratify/error-handler-module": "~0.1.1",
        "container-interop/definition-interop": "~0.2.0",
        "php-di/php-di": "dev-definition-interop@dev",
        "puli/composer-plugin": "^1.0",
        "puli/repository": "^1.0",
        "puli/discovery": "^1.0",
        "puli/url-generator": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "symfony/filesystem": "^3.0"
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}

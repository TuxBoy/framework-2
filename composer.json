{
    "name": "stratify/framework",
    "description": "Stratify framework package",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Stratify\\Framework\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Stratify\\Framework\\Test\\": "tests"
        }
    },
    "require": {
        "php": ">=7.0",
        "stratify/http": "~0.4.0",
        "stratify/router": "~0.4.0",
        "stratify/error-handler-module": "~0.3.0",
        "php-di/php-di": "^5.4.2",
        "php-di/kernel": "~0.3.0",
        "mnapoli/silly": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "symfony/filesystem": "^3.0"
    }
}
